<div class="main users-edit">
  <div class="container">
    <div class="form-heading">ユーザー情報の編集</div>
    <div class="form users-form">
      <div class="form-body">

      <% @user.errors.full_messages.each do |message| %>
          <div class="form-error">
            <%= message %>
          </div>
        <% end %>
<%= form_with(model: @user, local: true) do |f| %>  
<table>
  <tr>
  <td><%= f.label :name %></td>
  <td><%= f.text_field :name %></td>
  </tr>
  <tr>
  <td><%= f.label :email %></td>
  <td><%= f.email_field :email %></td>
  </tr>
  <tr>
  <td><%= f.label :password %></td>
  <td><%= f.password_field :password %></td>
  </tr>
  <tr>
  <td><%= f.label :password_confirmation %></td>
  <td><%= f.password_field :password_confirmation %></td>
  </tr>
  <tr>
  <td><%= f.label :プロフィール画像 %></td>
  <td><%= f.file_field :image_name %></td>
  <%= f.hidden_field :image_name_cache %>
  </tr>
</table>
  <td><%= f.submit "Create my account" %></td>
<% end %>
      </div>
    </div>
  </div>
</div>