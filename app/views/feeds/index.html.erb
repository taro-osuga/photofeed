<div class="container">
<p id="notice"><%= notice %></p>
<h1 button class="btn btn-primary" disabled="disabled">投稿一覧</h1>
<table class="table">
<table class="table table-bordered">
 <ul class="row">
    <% @feeds.each do |feed| %>
    <tbody>
        <td><%=image_tag feed.image.url(:thumb)%></td>
        <td><%=feed.comment %></td>
        <td><%=feed.user.name %>  <%=image_tag feed.user.image_name.url(:thumb)%></td>
        <td><button class="btn btn-default"><%= link_to 'Show', feed %></button></td>
        <% if feed.user_id == current_user.id %>
        <td><button class="btn btn-default"><%= link_to 'Edit', edit_feed_path(feed) %></button></td>
        <td><button class="btn btn-default"><%= link_to 'Destroy', feed, method: :delete, data: { confirm: 'Are you sure?' } %></button></td>
        <% end %>
        <% end %>
    </tbody>
   </ul>
    </table>
    </table>


</div>
